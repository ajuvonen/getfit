    "chart.js": "^4.4.3",
    "ics": "^3.7.2",
    "luxon": "^3.4.4",
    "pinia": "^2.1.7",
    "remeda": "^2.0.8",
    "sortablejs": "^1.15.2",
    "uuid": "^10.0.0",
    "vue": "3.4.27",
    "vue-chartjs": "^5.3.1",
    "vue-i18n": "^9.13.1",
