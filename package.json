    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vitest": "^1.0.1",
