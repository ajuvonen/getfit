{
  "name": "getfit",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:5173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 5173' http://localhost:5173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.ts --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vuepic/vue-datepicker": "^7.4.1",
    "@vueuse/core": "^10.7.2",
    "ics": "^3.7.2",
    "luxon": "^3.4.4",
    "pinia": "^2.1.7",
    "ramda": "^0.29.1",
    "resize-observer-polyfill": "^1.5.1",
    "uuid": "^9.0.1",
    "vue": "^3.4.15",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.2.5",
    "vuedraggable": "^4.1.0",
    "vuetify": "3.5.1"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@rushstack/eslint-patch": "^1.7.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.5",
    "@types/ramda": "^0.29.10",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.6.3",
    "eslint": "^8.56.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.20.1",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "start-server-and-test": "^2.0.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-vuetify": "^2.0.1",
    "vitest": "^1.2.1",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
