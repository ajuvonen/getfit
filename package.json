    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.5",
    "vue3-mq": "^3.1.3",
    "vuedraggable": "^4.1.0",
    "vuetify": "3.3.6"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@rushstack/eslint-patch": "^1.6.0",
